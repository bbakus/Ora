# Flask Configuration
FLASK_ENV=development
FLASK_APP=server.app
FLASK_DEBUG=1
SECRET_KEY=generate_a_random_secure_key_here
SERVER_PORT=5001

# Database Configuration
DATABASE_URL=sqlite:///app.db

# Google API Configuration
GOOGLE_MAPS_API_KEY=your_api_key_here
