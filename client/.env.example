# Google Maps API Key
# Get a key from https://console.cloud.google.com/
# Make sure to restrict this key to specific domains/IPs and only the Maps JavaScript API
REACT_APP_GOOGLE_MAPS_API_KEY=your_api_key_here

# Backend API URL - Update for different environments
REACT_APP_API_URL=http://localhost:5001

# Feature Toggles
REACT_APP_ENABLE_MOCK_API=false
REACT_APP_ENABLE_DEBUG_TOOLS=true 